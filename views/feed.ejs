<% include ./partials/header.ejs %>
    <div class="w-full min-h-screen bg-zinc-800 py-8 text-white ">
        <div class=" cards flex flex-wrap gap-10 m-5">
            <% posts.forEach(function(elem){ console.log(elem.image) %>
                <a href="/feed/<%= elem._id %>" class="inline-block">
                    <div class="w-32 card ">
                        <div class="w-full h-40 bg-sky-200 rounded-lg overflow-hidden">
                            <img class="w-full h-full object-cover" src="/images/uploads/<%= elem?.image %>">
                        </div>
                        <h5 class="mt-2">
                            <%= elem.title %>
                        </h5>
                    </div>
                </a>
                <% })%>

        </div>
    </div>
    <% include ./partials/footer.ejs %>